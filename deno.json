{
  "name": "@knorpelsenf/knorpelsolve",
  "version": "1.4.0",
  "exports": "./mod.ts",
  "license": "MIT",
  "lock": false,

  "permissions": {
    "default": {
      "sys": true,
      "read": true,
      "write": true,
      "net": ["jsr.io"],
      "ffi": true
    }
  },
  "tasks": {
    "ok": "deno fmt --check && deno lint && deno check *.ts",
    "build": "CARGO_TARGET_AARCH64_UNKNOWN_LINUX_GNU_LINKER=aarch64-linux-gnu-gcc cargo build --release --color=always --target x86_64-unknown-linux-gnu --target aarch64-unknown-linux-gnu"
  },
  "imports": {
    "@std/fs": "jsr:@std/fs@^1.0.19",
    "@std/path": "jsr:@std/path@^1.1.2"
  },
  "publish": {
    "include": [
      "./README.md",
      "./mod.ts",
      "./target/aarch64-unknown-linux-gnu/release/libknorpelsolve.so",
      "./target/x86_64-unknown-linux-gnu/release/libknorpelsolve.so"
    ],
    "exclude": [
      "!./target/aarch64-unknown-linux-gnu/release/libknorpelsolve.so",
      "!./target/x86_64-unknown-linux-gnu/release/libknorpelsolve.so"
    ]
  }
}
